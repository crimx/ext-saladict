{
  "env": {
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs",
          "targets": {
            "chrome": "55",
            "firefox": "56"
          }
        }],
        "react"
      ],
      "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true }]
      ]
    },
    "development": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "chrome": "55",
            "firefox": "56"
          }
        }],
        "react"
      ],
      "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true }]
      ]
    },
    "production": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "chrome": "55",
            "firefox": "56"
          }
        }],
        "react"
      ],
      "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true }]
      ]
    }
  }
}
